# git aliases
alias gd="git diff"
alias gs="git status"
alias ga="git add"
alias gaa="git add *"
alias gc="git commit"
alias gp="git pull"
alias gP="git push"
alias gl='git log --pretty=oneline --graph --decorate --abbrev-commit'

source ~/dotfile/bash/git-prompt.sh
source ~/dotfile/bash/git-completion.bash
